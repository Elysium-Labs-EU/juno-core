{
  "name": "juno-core",
  "version": "0.1.7",
  "description": "Main app for Juno",
  "main": "public/electron.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elysium-Labs-EU/juno-core.git"
  },
  "author": {
    "name": "Robbert Tuerlings",
    "email": "robberttg@protonmail.com",
    "url": "https://elysium-labs.io"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Elysium-Labs-EU/juno-core/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@mui/base": "^5.0.0-alpha.76",
    "@mui/material": "^5.6.1",
    "@mui/system": "^5.6.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "@tiptap/core": "^2.0.0-beta.176",
    "@tiptap/extension-blockquote": "^2.0.0-beta.26",
    "@tiptap/extension-bold": "^2.0.0-beta.26",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.26",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-hard-break": "^2.0.0-beta.30",
    "@tiptap/extension-heading": "^2.0.0-beta.26",
    "@tiptap/extension-italic": "^2.0.0-beta.26",
    "@tiptap/extension-list-item": "^2.0.0-beta.20",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.27",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/react": "^2.0.0-beta.109",
    "@tiptap/starter-kit": "^2.0.0-beta.185",
    "autolinker": "^3.15.0",
    "axios": "^0.27.0",
    "base64url": "^3.0.1",
    "date-fns": "^2.27.0",
    "dompurify": "^2.3.6",
    "file-saver": "^2.0.5",
    "framer-motion": "4.1.17",
    "history": "^5.3.0",
    "is-electron": "^2.2.1",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "pretty-bytes": "^6.0.0",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-first-history": "^5.0.8",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "electron:start": "electron-builder install-app-deps && concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn:electron\"",
    "electron:dir": "electron-builder --dir",
    "electron": "wait-on tcp:3000 && electron .",
    "release": "electron-builder --publish always",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "appId": "io.elysium-labs.juno",
    "productName": "Juno",
    "copyright": "Elysium Labs",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg",
      "artifactName": "${name}-${version}.${ext}"
    },
    "win": {
      "target": "nsis",
      "artifactName": "${name}-${version}.${ext}"
    },
    "linux": {
      "target": "deb",
      "artifactName": "${name}-${version}.${ext}"
    },
    "publish": [
      {
        "provider": "github",
        "releaseType": "release"
      }
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@mui/styled-engine": "yarn:@mui/styled-engine-sc@latest",
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/dompurify": "2.3.3",
    "@types/file-saver": "2.0.5",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.23",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.17",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.1.0",
    "concurrently": "7.0.0",
    "cross-env": "7.0.3",
    "electron": "18.0.3",
    "electron-builder": "23.0.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "13.0.0",
    "prettier": "2.6.2",
    "tsc-files": "1.1.3",
    "wait-on": "6.0.1",
    "webpack": "4.44.2"
  }
}
